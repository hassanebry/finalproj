{% extends 'base.html.twig' %}

{% block body %}
    <div class="form-group">
        <div class="col-sm-8">
            <h1>Reservation :</h1>

            {{ form_start(form, {attr: {class: 'filter'}}) }}
            {{ form_widget(form) }}
        </div>
    </div>
    <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10">
            <button type="submit" class="btn btn-primary">Reserver</button>
        </div>
    </div>
    {{ form_end(form) }}

{% endblock %}